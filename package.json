{
  "name": "create-application-template",
  "version": "2.3.1",
  "description": "provides a configured application template for you to build upon",
  "main": "index.js",
  "bin": {
    "create-application-template": "bin/create-application-template.js"
  },
  "scripts": {
    "start": "webpack serve --config webpack/webpack.config.js --env BUNDLER_ENV=dev",
    "build": "webpack --config webpack/webpack.config.js --env BUNDLER_ENV=prod BUNDLER_WITH_PROFILING=false",
    "build:profile": "webpack --config webpack/webpack.config.js --env BUNDLER_ENV=prod BUNDLER_WITH_PROFILING=true",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:updateSnapshot": "jest --updateSnapshot",
    "dev": "concurrently \"npm run test:watch\" \"npm run start\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "stylelint": "npx stylelint \"src/styles/**/*.{js,ts}\"",
    "stylelint:fix": "npx stylelint --fix \"src/styles/**/*.{js,ts}\"",
    "stylelint:css": "npx stylelint \"src/**/*.css\"",
    "stylelint:css:fix": "npx stylelint --fix \"src/**/*.css\""
  },
  "keywords": [
    "template",
    "react",
    "typescript",
    "styled-components",
    "webpack",
    "jest",
    "eslint",
    "stylelint",
    "javascript"
  ],
  "url": "https://www.createapplicationtemplate.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/daveKontro/create-application-template.git"
  },
  "author": "Dave Kontrovitz",
  "license": "MIT",
  "proxy": "http://localhost:3000",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "dependencies": {
    "browserslist": "4.24.4",
    "concurrently": "9.1.2",
    "dotenv-flow": "4.1.0",
    "husky": "9.1.7",
    "polished": "4.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "styled-components": "6.1.15",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.26.9",
    "@babel/plugin-transform-runtime": "7.26.9",
    "@babel/preset-env": "7.26.9",
    "@babel/preset-react": "7.26.3",
    "@babel/preset-typescript": "7.26.0",
    "@babel/runtime": "7.26.10",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.15",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "29.5.14",
    "@types/node": "20.12.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@types/styled-components": "5.1.34",
    "@types/webpack-env": "1.18.8",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "babel-jest": "29.7.0",
    "babel-loader": "10.0.0",
    "babel-plugin-styled-components": "2.1.4",
    "babel-plugin-transform-require-context": "0.1.1",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
