{
  "name": "create-application-template",
  "version": "0.11.0",
  "description": "provides a configured application template for you to build upon",
  "main": "index.js",
  "bin": {
    "create-application-template": "bin/create-application-template.js"
  },
  "scripts": {
    "start": "webpack serve --config webpack/webpack.config.js --env BUNDLER_ENV=dev",
    "build": "webpack --config webpack/webpack.config.js --env BUNDLER_ENV=prod BUNDLER_WITH_PROFILING=false",
    "build:profile": "webpack --config webpack/webpack.config.js --env BUNDLER_ENV=prod BUNDLER_WITH_PROFILING=true",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "concurrently \"npm run test:watch\" \"npm run start\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "stylelint": "npx stylelint \"src/styles/**/*.{js,ts}\"",
    "stylelint:fix": "npx stylelint --fix \"src/styles/**/*.{js,ts}\"",
    "stylelint:css": "npx stylelint \"src/**/*.css\"",
    "stylelint:css:fix": "npx stylelint --fix \"src/**/*.css\"",
    "prepare": "husky"
  },
  "keywords": [
    "template",
    "react",
    "typescript",
    "styled-components",
    "webpack",
    "jest",
    "eslint",
    "stylelint",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/daveKontro/create-application-template.git"
  },
  "author": "Dave Kontrovitz",
  "license": "MIT",
  "proxy": "http://localhost:3000",
  "engines": {
    "node": ">=18.19.1"
  },
  "dependencies": {
    "concurrently": "8.2.2",
    "dotenv-flow": "4.1.0",
    "husky": "9.0.11",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "styled-components": "6.1.12",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.24.5",
    "@babel/plugin-transform-runtime": "7.24.3",
    "@babel/preset-env": "7.24.5",
    "@babel/preset-react": "7.24.1",
    "@babel/preset-typescript": "7.24.1",
    "@babel/runtime": "7.24.5",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.13",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.7",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@types/styled-components": "5.1.34",
    "@types/webpack-env": "1.18.5",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "babel-jest": "29.7.0",
    "babel-loader": "9.1.3",
    "babel-plugin-transform-require-context": "0.1.1",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "confusing-browser-globals": "1.0.11",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "7.0.0",
    "dotenv-webpack": "8.1.0",
    "eslint": "8.56.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-webpack-plugin": "5.6.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-extended": "4.0.2",
    "jest-styled-components": "7.2.0",
    "jest-watch-typeahead": "2.2.2",
    "mini-css-extract-plugin": "2.9.0",
    "postcss-styled-components": "0.2.1",
    "react-refresh": "0.14.2",
    "style-loader": "4.0.0",
    "stylelint": "16.5.0",
    "stylelint-config-recess-order": "5.0.1",
    "stylelint-config-standard": "36.0.0",
    "terser-webpack-plugin": "5.3.10",
    "typescript": "5.4.5",
    "webpack": "5.94.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.4",
